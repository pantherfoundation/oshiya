  "scripts": {
    "start": "webpack-cli serve --mode=development --env development --hot",
    "check-types": "tsc --project tsconfig.json --noEmit --isolatedModules false",
    "build:dev": "yarn check-types && TARGET_ENV=development  webpack --mode=development --env development",
    "build:staging": "yarn check-types && TARGET_ENV=staging yarn webpack --mode=production --env production --progress",
    "build:prod": "yarn check-types && TARGET_ENV=production webpack --mode=production --env production --progress",
    "bundle:wasm": "cp -R ../sdk/src/wasm/* ./build/",
    "deploy:ipfs:infura": "ts-node scripts/deploy-ipfs.ts --infura"
  },
